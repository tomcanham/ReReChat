{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch localhost:3000",
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "url": "http://localhost:3000/index.html",
      "webRoot": "${workspaceFolder}/public",
      "pathMappings": [
        {
          "url": "http://localhost:3000/home",
          "path": "vscode-remote://wsl%2Bdebian/home"
        },
        {
          "url": "webpack:///~/",
          "path": "${webRoot}/node_modules/"
        },
        {
          "url": "webpack:///./~/",
          "path": "${webRoot}/node_modules/"
        },
        {
          "url": "webpack:///./",
          "path": "${webRoot}/"
        },
        {
          "url": "webpack:///src/",
          "path": "${webRoot}/client/src/"
        },
        {
          "url": "webpack:///node_modules/",
          "path": "${webRoot}/node_modules/"
        },
        {
          "url": "webpack:///webpack",
          "path": null
        },
        {
          "url": "webpack:///(webpack)",
          "path": null
        },
        {
          "url": "webpack:///pages/",
          "path": "${webRoot}/pages/"
        },
        {
          "url": "webpack://[name]_[chunkhash]/node_modules/",
          "path": "${webRoot}/node_modules/"
        },
        {
          "url": "webpack://[name]_[chunkhash]/",
          "path": null
        },
        {
          "url": "webpack:///",
          "path": ""
        }
      ]
    }
  ]
}